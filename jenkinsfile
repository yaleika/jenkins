pipeline {

agent any

tools {

nodejs "node"

}



   parameters {
    string(name: 'STATEMENT', defaultValue: 'hello; ls /', description: 'What should I say?')
  }




stages{

    stage ('install'){
        steps {
            git branch: 'main', url: 'https://github.com/yaleika/jenkins.git'
            dir ('JENKIS'){ sh 'npm install'}
            echo "hola mundo"
        }
    }

}

}



